<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:shape="oval">
        <shape android:shape="oval">
            <solid android:color="#F2F2F2" />
        </shape>
    </item>

    <item
        android:width="23dp"
        android:height="23dp"
        android:top="1dp"
        android:left="1dp"
        android:bottom="1dp"
        android:right="1dp"
        android:shape="oval">
        <shape android:shape="oval">
            <solid android:color="#F2F2F2" />
            <stroke
                android:width="2dp"
                android:color="#FFFFFF" />
            <size
                android:width="24dp"
                android:height="24dp" />
        </shape>
    </item>



</layer-list>